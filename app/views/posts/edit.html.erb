<div class="padding">
  <h1>Edit Blog Post</h1>

  <%= bootstrap_form_for @post do |f| %>
    <%= f.text_field :post_title %>
    <br><br>
    <%= f.text_area :post_content %>
    <br>
    <%= f.file_field :attached_image %>
    <br>
    <%= f.submit "Edit post", class: "btn btn-success" %>
  <% end %>
</div>
